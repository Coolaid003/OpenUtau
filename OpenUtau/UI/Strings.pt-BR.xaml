    <!-- Dialogs -->
    <system:String x:Key="dialogs.noresampler.caption">Nenhum resampler selecionado</system:String>
    <system:String x:Key="dialogs.noresampler.message">Nenhum resampler selecionado! Coloque seu resampler exe ou dll favorito na pasta Resamplers e escolha-o em Preferências!</system:String>
    <system:String x:Key="dialogs.about.caption">Sobre OpenUTAU</system:String>
    <system:String x:Key="dialogs.about.message">
        OpenUtau pretende ser um ambiente de edição de código aberto para a comunidade UTAU, com uma experiência de usuário moderna e suporte fonológico inteligente. Visite-nos no Github!
    </system:String>
    <system:String x:Key="dialogs.export.caption">Exportar</system:String>
    <system:String x:Key="dialogs.export.savefirst">Salve o projeto primeiro.</system:String>

