    <system:String x:Key="tracks.remove">Remover</system:String>
    <system:String x:Key="tracks.moveup">Mover Pra Cima</system:String>
    <system:String x:Key="tracks.movedown">Mover Pra Baixo</system:String>

    <!-- Errors -->
    <system:String x:Key="errors.caption">Erro</system:String>
    <system:String x:Key="warning">Aviso</system:String>
    <system:String x:Key="warning.asciipath">Seu diretório de arquivos do OpenUtau "{0}" contém caracteres não-ASCII. Os resamplers Exe podem não funcionar.</system:String>

